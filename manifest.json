{
  "manifest_version": 2,
  "name": "ydwd",
  "version": "1.0",
  "description": "Query words by youdao API",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}